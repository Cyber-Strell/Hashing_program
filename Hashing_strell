import hashlib
mystring = input('Enter Your word to hash: ')
print("""""""""""""""""""""""""""""""""""""""""""""""")
hash_obj = hashlib.md5(mystring.encode())
hash_obj1 = hashlib.sha256(mystring.encode())
print('Your Md5 encrypted Result :', hash_obj.hexdigest())
print('Your sha256 Result :', hash_obj1.hexdigest())
print("""""""""""""""""""""""""""""""""""""""""""""""")


